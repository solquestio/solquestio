{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/quests/paths",
      "dest": "/api/quests/paths.ts"
    },
    {
      "src": "/api/quests/\\[id\\]",
      "dest": "/api/quests/[id].ts"
    },
    {
      "src": "/api/quests/complete",
      "dest": "/api/quests/complete.ts",
      "methods": ["POST"]
    },
    {
      "src": "/api/auth/challenge",
      "dest": "/api/auth/challenge.ts",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/auth/verify",
      "dest": "/api/auth/verify.ts",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/users",
      "dest": "/api/users/index.ts",
      "methods": ["GET", "POST", "PUT"]
    },
    {
      "src": "/api/utils",
      "dest": "/api/utils.ts"
    },
    {
      "src": "/api/public",
      "dest": "/api/public.ts"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth/index.ts"
    },
    {
      "src": "/api/quests",
      "dest": "/api/quests/index.ts"
    },
    {
      "src": "/api",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}